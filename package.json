{
    "name": "winston-transport-prometheus",
    "version": "0.1.0",
    "description": "A winston metrics transport using Prometheus",
    "author": "Manabu Matsuzaki",
    "license": "Apache-2.0",
    "homepage": "https://github.com/matsumana/winston-transport-prometheus",
    "repository": {
        "type": "git",
        "url": "git@github.com:matsumana/winston-transport-prometheus.git"
    },
    "engines": {
        "node": ">=12"
    },
    "keywords": [
        "winston",
        "prometheus",
        "metrics"
    ],
    "main": "lib/index.ts",
    "scripts": {
        "fix:prettier": "prettier --write {lib,test}",
        "fix:eslint": "eslint --ext .js,.ts --fix {lib,test}",
        "fix": "run-s fix:prettier fix:eslint",
        "lint:prettier": "prettier --check {lib,test}",
        "lint:eslint": "eslint --ext .js,.ts {lib,test}",
        "lint": "run-p lint:prettier lint:eslint",
        "test": "jest",
        "clean": "rm -rf dist",
        "build": "tsc"
    },
    "dependencies": {
        "winston": "^3.3.3",
        "winston-transport": "^4.4.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.41",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.25.0",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "prom-client": "^13.1.0",
        "ts-jest": "^26.5.5",
        "ts-loader": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "peerDependencies": {
        "prom-client": "^13.1.0"
    }
