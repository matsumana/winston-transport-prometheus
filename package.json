{
    "name": "@matsumana/winston-transport-prometheus",
    "version": "1.0.1",
    "description": "A winston metrics transport using Prometheus",
    "author": "Manabu Matsuzaki",
    "license": "Apache-2.0",
    "homepage": "https://github.com/matsumana/winston-transport-prometheus",
    "repository": {
        "type": "git",
        "url": "git@github.com:matsumana/winston-transport-prometheus.git"
    },
    "engines": {
        "node": ">=12"
    },
    "keywords": [
        "winston",
        "prometheus",
        "metrics"
    ],
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "fix:prettier": "prettier --write .",
        "fix:eslint": "eslint --ext .js,.ts --fix .",
        "fix": "run-p -l -c --aggregate-output 'fix:*'",
        "lint:prettier": "prettier --check .",
        "lint:eslint": "eslint --ext .js,.ts .",
        "lint": "run-p -l -c --aggregate-output 'lint:*'",
        "test": "jest",
        "clean": "rm -rf dist",
        "build": "tsc"
    },
    "dependencies": {
        "winston": "^3.3.3",
        "winston-transport": "^4.4.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.41",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^26.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "prom-client": "^13.1.0",
        "ts-jest": "^26.5.5",
        "typescript": "^4.3.2"
    },
    "peerDependencies": {
        "prom-client": "^13.1.0"
    }
}
